title: /VIM/
-

# Captures

capture letter:
  user.vim_insert_mode(" <user.letter>")
  
capture letters:
  user.vim_insert_mode(" <user.letters>")

capture word:
  user.vim_insert_mode(" <user.word>")
  
capture text:
  user.vim_insert_mode(" <user.text>")

capture number:
  user.vim_insert_mode(" <user.number_string>")
