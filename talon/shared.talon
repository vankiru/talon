title: /VIM/
-

# Shared talon commands for .py and .talon files

(talon | talent) letter:
  user.vim_insert_mode(" <user.letter>")
  
(talon | talent) letters:
  user.vim_insert_mode(" <user.letters>")

(talon | talent) word:
  user.vim_insert_mode(" <user.word>")
  
(talon | talent) text:
  user.vim_insert_mode(" <user.text>")
